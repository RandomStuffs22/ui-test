<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Platform Sniff Test" verbose="1" preserve-order="true">
	<test name="Update users" enabled="true">
		<groups>
			<run>
				<exclude name="pending" />
			</run>
		</groups>
		<classes>
			<class name="org.exoplatform.selenium.community.Community_Create_Users">
        		<methods>
          			<include name="test02_UpdateUserAccount"/>
          			<include name="test03_UpdateMembership"/>
        		</methods>
      		</class>
		</classes>
	</test>
	<listeners>
		<listener class-name="org.exoplatform.selenium.TestReportListener" />
	</listeners>
</suite>